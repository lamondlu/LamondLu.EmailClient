@model LamondLu.EmailX.Domain.Models.PagedResult<LamondLu.EmailX.Domain.ViewModels.EmailListViewModel>


 
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Subject</th>
      <th scope="col">From</th>
      <th scope="col">Received Date</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
  @foreach (var item in Model.Data)
  {
    <tr>
      <th scope="row">@item.Id</th>
      <td>@item.Subject</td>
      <td>@item.Sender</td>
      <td>@item.ReceiveDate.ToString("yyyy-MM-dd")</td>
      <td></td>
    </tr>
  }
  
  </tbody>
</table>